<% @posts.each do |post|%>
    <div>
        <h2><%= post.title %></h2>
        <p><%= post.body %></p>
        <% if user_signed_in? %>
            <p>By: <%= post.user.email %><p>
        <%end%>
    </div>
<%end%>

<% if user_signed_in? %>
  <%= button_to "Sign out", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "Sign in", new_user_session_path %>
<% end %>
